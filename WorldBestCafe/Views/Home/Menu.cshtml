
@{
    ViewBag.Title = "Menu";
}

<h2>Menu</h2>

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-primary list-panel" id="list-panel">
            <div class="panel-heading list-panel-heading">
                <h1 class="panel-title list-panel-title">Menu</h1>
            </div>
            <div class="panel-body">
                <table id="assets-data-table"
                       class="table table-striped table-bordered"
                       style="width:100%">
                    <thead>
                        <tr>
                            <th>Menu</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var Item in ViewBag.Menu)
                        {
                            <tr>
                                <td>@Item</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>